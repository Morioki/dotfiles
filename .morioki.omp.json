{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "root <#ffffff>in</> ",
          "type": "root"
        },
        {
          "foreground": "#55B9C4",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#C678DD",
          "properties": {
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        },
        {
          "type": "executiontime",
          "style": "plain",
          "template": " ({{ .FormattedMs }})",
          "properties": {
            "threshold": 500,
            "style": "austin"
          }
        },
        {
          "foreground": "#C94A16",
          "style": "plain",
          "template": "x ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#98C379",
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": true,
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#ffffff>via</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }}{{ .Full }}",
          "type": "node"
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "#193549",
          "background": "#99908a",
          "template": "<#ffffff>via</> \uE7a8 {{ .Full }} "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#007d9c",
          "template": " \uFCD1 {{ .Full }} "
        },
        {
          "foreground": "#FED142",
          "properties": {
            "display_mode": "context"
          },
          "style": "plain",
          "template": "<#ffffff>via</> \uf820{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "python"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#63F08C",
          "style": "plain",
          "template": "\u279c ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
